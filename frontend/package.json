{
  "name": "cassiopee-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run clean-log && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --config .prettierrc.json --write ./src",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev --name init",
    "deploy": "prisma migrate deploy",
    "reset": "prisma migrate reset --force",
    "seed": "prisma db seed",
    "clean-log": "rm -rf ../logs/**/*.log",
    "test": "vitest"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.14.1",
    "@tanstack/react-query": "^4.29.1",
    "@tanstack/react-query-devtools": "^4.29.7",
    "@zxcvbn-ts/core": "^2.2.1",
    "@zxcvbn-ts/language-common": "^2.0.1",
    "@zxcvbn-ts/language-fr": "^2.2.0",
    "autoprefixer": "^10.4.14",
    "chalk": "^5.2.0",
    "crypto-js": "^4.1.1",
    "ky": "^0.33.3",
    "ky-universal": "^0.11.0",
    "loglevel": "^1.8.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "next": "~13.3.0",
    "next-auth": "^4.22.1",
    "nodemailer": "^6.9.2",
    "postcss": "^8.4.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "sass": "^1.62.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "zod": "^3.21.4",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.16.16",
    "@types/nodemailer": "^6.4.8",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.4",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "prisma": "^4.14.1",
    "ts-node": "^10.9.1",
    "vitest": "^0.31.2"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}